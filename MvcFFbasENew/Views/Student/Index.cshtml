@model IEnumerable<FFbasEDataEF.STUDENT>

@{
    ViewBag.Title = "Index";
}

<h2>Список студентов</h2>

<p>
    @Html.ActionLink("Добавить нового студента", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.S_C)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.S_CL)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CLIENT.C_NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.COURSE_OF_LECTURE.CL_THEME)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.S_ID  }) |
            @Html.ActionLink("Details", "Details", new { id = item.S_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.S_ID })
        </td>
    </tr>
}

</table>
